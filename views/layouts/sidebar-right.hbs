</head>
<body hoe-navigation-type="vertical" hoe-nav-placement="right" theme-layout="wide-layout" >
    <div id="hoeapp-wrapper" class="hoe-hide-lpanel" hoe-device-type="desktop">
        <header id="hoe-header" hoe-color-type="logo-bg8" hoe-lpanel-effect="shrink">
            <div class="hoe-left-header">
                <a href="#"><span class="logo">{{{__ "Plexus"}}}</span></a>
                <span class="hoe-sidebar-toggle"><a href="#"></a></span>
            </div>
            <div class="hoe-right-header" hoe-color-type="header-bg8" hoe-position-type="relative" >
                <span class="hoe-sidebar-toggle"><a href="#"></a></span>					
            </div>
        </header>
        <div id="hoeapp-container" hoe-color-type="lpanel-bg8" hoe-lpanel-effect="shrink">
            <aside id="hoe-left-panel" hoe-position-type="absolute">
            	{{> sidebarmenu }}
            </aside>
            <section id="main-content">    
                <div class="content-title" style="display:{{showtitle}}">
                    <h3 class="main-title">{{title}}</h3>
                    <span>{{subtitle}}</span>
                </div>
                <div class="inner-content">            
                    {{{body}}}
                </div>
            </section>
        </div>
    </div>
  {{> footer}}
	<script src="/javascript/frontend.js"></script>
</body>
</html>